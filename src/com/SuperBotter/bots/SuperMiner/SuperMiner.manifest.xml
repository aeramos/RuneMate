<manifest>
    <main-class>com.SuperBotter.bots.SuperMiner.SuperMiner</main-class>
    <name>SuperMiner</name>
    <tag-line>The super mining bot</tag-line><!--Max of 50 chars-->
    <description>A super mining bot that banks or powermines with the Action Bar in many locations with more to come!</description><!--Max of 110 chars-->
    <version>0.5.6</version>
    <compatibility>
        <game>RS3</game>
    </compatibility>
    <categories>
        <category>MINING</category>
        <category>MONEYMAKING</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>SuperMiner</internal-id>
    <!--The rest are optional-->
    <open-source>false</open-source>
    <hidden>false</hidden> <!--If you need to hide it from the bot store for maintenance-->
    <access>public</access>
    <resources>
        <resource>com/SuperBotter/api/ui/Config.fxml</resource>
        <resource>com/SuperBotter/api/ui/Info.fxml</resource>
    </resources>
</manifest>